<% include ./partials/header %>

  <!-- Main -->
  <div class="page-content" style="padding-top:50px">
  <div id="wrapper" class="fade-in">
    <!-- Main -->
    <div id="main">
    <!-- Post -->
      <section class="post">
        <header class="major">
          <span class="date"><%= moment(event.date).format("MMM Do YY") %></span>
            <h1><%= event.title %></h1>
        </header>
        <div class="image main">
          <img src="https://images.unsplash.com/photo-1466150036782-869a824aeb25?auto=format&fit=crop&w=1050&q=60&ixid=dW5zcGxhc2guY29tOzs7Ozs%3D" alt="" />
        </div>
        <p><%= event.content %></p>
      </section>
    </div>
  </div>
  </div>

<% include ./partials/footer %>
